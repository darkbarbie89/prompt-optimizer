{
  "buildCommand": "pnpm install && pnpm --filter web build",
  "outputDirectory": "packages/web/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_VERCEL_DEPLOYMENT": "true"
  }
}
